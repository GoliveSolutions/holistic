 "html": "<div >\n    {% if letter_head and not no_letterhead -%}\n      <div class=\"letter-head\">{{ letter_head }}</div>\n      <hr>\n    {%- endif %}\n  \n    <div><h2 class=\"text-uppercase text-center\"><b>{{ _(\"\u062c\u062f\u0648\u0644 \u0645\u0648\u0627\u0639\u064a\u062f \u0627\u0644\u0645\u0631\u0636\u0649\") }}</b></h2></div>\n    <div><h2 class=\"text-uppercase text-center\"><b>Patient schedule appointments</b></h2></div>\n      <br/>\n      <div class=\"row section-break\">\n  \n        <div class=\"row\">\n          <div class=\"col-xs-3 test-left\">\n            <label>&nbsp;&emsp;Name</label>\n          </div>\n          {% if doc.patient_name %}\n          <div class=\"col-xs-6  test-left value\">\n            {{ doc.patient_name }}\n          </div>\n          {% else %}\n          <div class=\"col-xs-6  test-left value\">\n            {{ doc.patient }}\n          </div>\n          {%- endif -%}\n          <div class=\"col-xs-3 test-left\">\n            <label>{{ _(\"\u0627\u0644\u0627\u0633\u0645\") }}</label>\n          </div>          \n        </div>\n  \n        <div class=\"row\">\n          <div class=\"col-xs-3 test-left\">\n            <label>&nbsp;&emsp;File No</label>\n          </div>\n          <div class=\"col-xs-6  value test-left\">\n            {{ doc.name }}\n          </div>\n          <div class=\"col-xs-3 test-left\">\n            <label>{{ _(\"\u0631\u0642\u0645 \u0627\u0644\u0645\u0644\u0641\") }}</label>\n          </div>          \n        </div>\n\n        <div class=\"row\">\n          <div class=\"col-xs-3 test-left\">\n            <label>&nbsp;&emsp;Age</label>\n          </div>\n          <div class=\"col-xs-6  value test-left\">\n            {{  doc.patient_age_str }}\n          </div>\n          <div class=\"col-xs-3 test-left\">\n            <label>{{ _(\"\u0627\u0644\u0639\u0645\u0631\") }}</label>\n          </div>          \n        </div>\n  \n        <div class=\"row\">\n          <div class=\"col-xs-3 test-left\">\n            <label>&nbsp;&emsp;Gender</label>\n          </div>\n          <div class=\"col-xs-6  value test-left\">\n            {{ doc.patient_sex or '' }}\n          </div>\n          <div class=\"col-xs-3 test-left\">\n            <label>{{ _(\"\u0627\u0644\u062c\u0646\u0633\") }}</label>\n          </div>          \n        </div>\n  \n        <div class=\"row\">\n          <div class=\"col-xs-3 test-left\">\n            <label>&nbsp;&emsp;Date</label>\n          </div>\n          <div class=\"col-xs-6  value test-left\">\n            {{frappe.format_date(doc.creation)}}\n          </div>\n          <div class=\"col-xs-3 test-left\">\n            <label>{{ _(\"\u0627\u0644\u062a\u0627\u0631\u064a\u062e\") }}</label>\n          </div>          \n        </div>\n        \n        <div class=\"row\">\n          <div class=\"col-xs-3 test-left\">\n            <label>&nbsp;&emsp;Referral doctor</label>\n          </div>\n          <div class=\"col-xs-6  value test-left\">\n            {{ doc.practitioner or '' }}\n          </div>\n          <div class=\"col-xs-3 test-left\">\n            <label>{{ _(\"\u062a\u062d\u0648\u064a\u0644 \u0627\u0644\u0637\u0628\u064a\u0628 \") }}</label>\n          </div>          \n        </div>        \n    </div>\n  \n\n  \n      <table class=\"table table-bordered\">\n      <tbody>\n        {%- if doc.all_appointments -%}\n        <tr>\n          <th  class=\"text-left\">Time</th>\n          <th class=\"text-left\">Date</th>\n          <th class=\"text-left\">Service</th>\n          <th class=\"text-right\">Day</th>\n        </tr>\n  \n\n        {%- for row in doc.all_appointments -%}\n        <tr>\n          <td style=\"width: 25%;text-align: left\">\n            {%- if row.appointment_time -%}&nbsp;{{ row.appointment_time }}{%- endif -%}\n          </td>\n  \n          <td style=\"width: 25%;text-align: left\">\n            {%- if row.appointment_date -%}&nbsp;{{ frappe.format( row.appointment_date , {'fieldtype': 'Date'}) }}{%- endif -%}\n          </td>\n  \n          <td style=\"width: 30%;text-align: left\">\n            <div style=\"border: 0px;\">\n              {%- if row.service_unit -%}{{ row.service_unit }}{%- endif -%}\n            </div>\n          </td>\n\n          <td style=\"width: 20%;text-align: right\">\n            <div style=\"border: 0px;\">\n              {%- if row.day -%}{{ row.day }}{%- endif -%}\n            </div>\n          </td>          \n        </tr>\n  \n        {%- endfor -%}\n        {%- endif -%}\n      </tbody>\n    </table>\n  \n\n\n  </div>",
